<h1>Meetings</h1>

<table>
  <thead>
    <tr>
      <th>Meeting ID</th>
      <th>Meeting Title</th>
      <th>Meeting Start Time</th>
      <th>Meeting End Time</th>
    </tr>
  </thead>

  <tbody>
    <% @meetings.each do |meeting| %>
      <tr>
        <td><%= meeting.id %></td>
        <td><%= meeting.title %></td>
        <td><%= meeting.start_time %></td>
        <td><%= meeting.end_time %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', :back %>
