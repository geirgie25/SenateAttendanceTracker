<<h1>Meetings</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Meeting ID</th>
      <th scope="col">Meeting Title</th>
      <th scope="col">Meeting Start Time</th>
      <th scope="col">Meeting End Time</th>
    </tr>
  </thead>

  <tbody>
    <% @meetings.each do |meeting| %>
      <tr>
        <td><%= meeting.id %></td>
        <td><%= meeting.title %></td>
        <td><%= meeting.start_time %></td>
        <td><%= meeting.end_time %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', :back %>
