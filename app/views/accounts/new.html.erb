<h1>SIGN UP</h1>
<p>Find me in app/views/accounts/new.html.erb</p>

<%= form_for @account do |temp| %>
  <%= temp.label :username %>
  <br>
  <%= temp.text_field :username %>
  <br>
  <br>
  <%= temp.label :password %>
  <br>
  <%= temp.password_field :password %>
  <br>
  <br>
  <%= temp.submit %>
