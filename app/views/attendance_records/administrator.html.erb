<h2> Meeting Details</h2>

<table>
  <thead>
    <tr>
      <th>Meeting ID: </th>
      <th>Start Time: </th>
      <th>End Time: </th>
      <th>Meeting Link: </th>
    </tr>
  </thead>
  <tbody>
    <% @meetings.each do |meeting| %>
    <tr>
        <td> <%= meeting.id %></td>
        <td> <%= meeting.start_time %></td>
        <td> <%= meeting.end_time %></td>
        <td> <%= link_to "Show Meeting", "/dashboard/admin/" + meeting.id.to_s %> </td>
    </tr>
    <% end %>
  </tbody>
</table>
